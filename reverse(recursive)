#include <stdio.h>
int main()
{
    int size;
    scanf("%d",&size);
    int arr[size];
    for(int i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    int start=0,end=size-1;
    reverse(arr,start,end);
    for(int i=0;i<size;i++)
    {
        printf("%d",arr[i]);
    }
    return 0;
}
void reverse(int arr[],int start,int end)
{   
    if(start>end)
    {
        return;
    }
    int temp;
    temp=arr[start];
    arr[start]=arr[end];
    arr[end]=temp;
    start++;end--;
    reverse(arr,start,end);
}
