#include <stdio.h>
#include <stdlib.h>
int main()
{
   int size,rotations;
   printf("Enter the number of elements:");
   scanf("%d",&size);
   int arr[size];
   printf("Enter the elements:");
   for(int i=0;i<size;i++)
   {
	 scanf("%d",&arr[i]);
   }
   printf("Enter number of rotations:");
   scanf("%d",&rotations);
   for(int i=0;i<rotations;i++)
   {
      int last=arr[size-1];
      for(int j=size-1;j>=0;j--)
      {
	   arr[j]=arr[j-1];
      }
      arr[0]=last;
   }
   printf("Array after rotation:");
   for(int i=0;i<size;i++)
   {
      printf("%d ",arr[i]);
   }
}
