#include <stdio.h>
#include<stdlib.h>
void swap(int *x,int *y)
{
    int temp=*x;
    *x=*y;
    *y=temp;
}
int partition(int arr[],int low,int high)
{  int pivot=arr[low];
   int i=low-1;
   int j=high+1;
   while(1)
   {
       do{
           i++;
       }while(arr[i]<pivot);
       do{
           j--;
       }while(arr[j]>pivot);
       if(i>=j)
       
           return j;
           swap(&arr[i],&arr[j]);
   }
    
    
}
void quickSort(int arr[],int low,int high)
{   if(low<high){
    int pi=partition(arr,low,high);
    quickSort(arr,low,pi);
    quickSort(arr,pi+1,high);
}
}
void printArray(int arr[],int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d",arr[i]);
    }
}
int main()
{ int arr[]={4,2,7,1,9,8,5};
  int n=sizeof(arr)/sizeof(arr[0]);
  
  quickSort(arr,0,n-1);
  printArray(arr,n);

    return 0;
}
